pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hellllo') {
            steps {
                echo 'Hello World'
            }
        }
        stage('sudo') {
            steps {
                sh '''sudo su'''
            }
        }
        stage('ShellBush') {
            steps {
                sh '''bash run.sh'''
            }
        }
    }
}
